{
	"version": 1,
	"base_image": "codeocean/jupyterlab:3.6.1-miniconda4.12.0-python3.9-ubuntu20.04",
	"options": {
		"registry_host_arg": true,
		"git_ask_pass": true
	},
	"installers": {
		"conda": {
			"packages": [
				{
					"name": "matplotlib",
					"version": "3.8.0"
				},
				{
					"name": "numpy",
					"version": "1.26.4"
				},
				{
					"name": "pandas",
					"version": "2.1.4"
				},
				{
					"name": "plotly",
					"version": "5.19.0"
				},
				{
					"name": "s3fs",
					"version": "2023.10.0"
				},
				{
					"name": "scikit-learn",
					"version": "1.5.1"
				}
			]
		},
		"pip": {
			"packages": [
				{
					"name": "pynwb",
					"version": "2.6.0"
				},
				{
					"name": "seaborn",
					"version": "0.13.2"
				},
				{
					"name": "statsmodels",
					"version": "0.14.2"
				}
			],
			"options": {},
			"pre_install_options": {}
		}
	}
}