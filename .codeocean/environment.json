{
	"version": 1,
	"base_image": "codeocean/cuda-miniconda-jupyterlab:latest",
	"options": {
		"registry_host_arg": true,
		"git_ask_pass": true
	},
	"installers": {
		"conda": {
			"packages": [
				{
					"name": "matplotlib",
					"version": "3.9.2"
				},
				{
					"name": "numpy",
					"version": "1.26.4"
				},
				{
					"name": "pandas",
					"version": "2.2.3"
				},
				{
					"name": "python",
					"version": "3.10.16"
				},
				{
					"name": "scikit-learn",
					"version": "1.5.2"
				},
				{
					"name": "scipy",
					"version": "1.14.1"
				},
				{
					"name": "seaborn",
					"version": "0.13.2"
				},
				{
					"name": "statsmodels",
					"version": "0.14.4"
				}
			]
		},
		"pip": {
			"packages": [
				{
					"name": "aind-dynamic-foraging-basic-analysis",
					"version": "0.3.7"
				},
				{
					"name": "aind-dynamic-foraging-data-utils",
					"version": "0.1.3"
				},
				{
					"name": "joblib"
				},
				{
					"name": "pynwb",
					"version": "2.8.3"
				}
			],
			"options": {},
			"pre_install_options": {}
		}
	}
}